<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- menu contact -->
    <menuitem id="t4_contact_menu" name="T4 Contact" />
    <record id="action_t4_contact" model="ir.actions.act_window">
        <field name="name">Contact</field>
        <field name="res_model">res.partner</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="menu_contact" name="Contact" parent="t4_contact_menu" action='action_t4_contact' />

    <!-- menu infor -->
    <record id="action_info" model="ir.actions.act_window">
        <field name="name">Info</field>
        <field name="res_model">res.partner</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="menu_info" name="Info" parent="menu_contact" action="action_info" />
    <!-- menu email -->
    <record id="action_biz_email" model="ir.actions.act_window">
        <field name="name">Email</field>
        <field name="res_model">biz.email</field>
        <field name="view_mode">tree,form</field>

    </record>
    <menuitem id="contact_email" name="Email" parent="menu_info" action="action_biz_email" />
    <!-- menu phone -->

    <record id="action_biz_phone" model="ir.actions.act_window">
        <field name="name">Phone</field>
        <field name="res_model">biz.phone</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="contact_phone" name="Phone" parent="menu_info" action="action_biz_phone" />

    <!-- menu fax -->

    <record id="action_biz_fax" model="ir.actions.act_window">
        <field name="name">Fax</field>
        <field name="res_model">biz.fax</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="contact_fax" name="Fax" parent="menu_info" action="action_biz_fax" />
    <!-- menu website -->

    <record id="action_biz_website" model="ir.actions.act_window">
        <field name="name">Website</field>
        <field name="res_model">biz.website</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="contact_website" name="Website" parent="menu_info" action="action_biz_website" />

</odoo>