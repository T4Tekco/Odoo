<template xml:space ="preserve">
    <div t-name="t4_statistic.s_t4_statistic" >
        <t t-foreach="keyword" t-as="result">
                  <div class="container">
                   
                    <table style="width:30%; border: 1px solid black">
                          <tr>
                             <td> <t t-out="result.name"/> </td>
                            <td> <t t-out="result.search_portal_count"/> </td>
                          </tr>
                    </table>  
                  </div>
        </t>
    </div>
</template>